{
  "name": "ksp",
  "version": "1.0.0",
  "description": "restAPI project koperasi simpan pinjam",
  "main": "index.js",
  "scripts": {
    "demon": "nodemon --delay 1.3 app.js",
    "test": "cross-env NODE_ENV=test jest --coverage --testTimeOut=8000 --detectOpenHandles --forceExit",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "first": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npm run first",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ayysz/backend-ksp.git"
  },
  "author": "player",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ayysz/backend-ksp/issues"
  },
  "homepage": "https://github.com/Ayysz/backend-ksp#readme",
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cookiejar": "^2.1.3",
    "cross-env": "^7.0.3",
    "jest": "^29.0.2",
    "supertest": "^6.2.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "helmet": "^6.0.0",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "middleware-stack-printer": "^0.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.19",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "^6.21.4",
    "sequelize-cli": "^6.4.1"
  }
}
